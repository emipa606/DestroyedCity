<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="BuildingBase">
    <defName>Plane</defName>
    <label>air plane crash</label>
    <thingClass>Building</thingClass>
    <category>Building</category>
    <description>An old crash site, deconstruct for resources.</description>
    <graphicData>
      <texPath>Things/Building/plane_Atlas</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(10,10)</drawSize>
      <damageData>
        <rect>(0,3.5,10,30)</rect>
      </damageData>
      <shadowData>
        <volume>(20, 6, 3)</volume>
      </shadowData>
    </graphicData>
    <size>(10,10)</size>
    <selectable>true</selectable>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <filthLeaving>Filth_RubbleBuilding</filthLeaving>
    <statBases>
      <MaxHitPoints>1000</MaxHitPoints>
      <WorkToMake>9000</WorkToMake>
      <Flammability>.1</Flammability>
      <Beauty>-10</Beauty>
    </statBases>
    <mineable>false</mineable>
    <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <blockWind>true</blockWind>
    <castEdgeShadows>false</castEdgeShadows>
    <coversFloor>false</coversFloor>
    <tickerType>Never</tickerType>
    <neverMultiSelect>true</neverMultiSelect>
    <holdsRoof>false</holdsRoof>
    <blockLight>true</blockLight>
    <costList>
      <Steel>500</Steel>
      <ComponentIndustrial>20</ComponentIndustrial>
    </costList>
    <building>
      <isInert>true</isInert>
      <ai_chillDestination>false</ai_chillDestination>
      <isEdifice>false</isEdifice>
    </building>
    <damageMultipliers>
      <li>
        <damageDef>Bomb</damageDef>
        <multiplier>1</multiplier>
      </li>
    </damageMultipliers>
  </ThingDef>
